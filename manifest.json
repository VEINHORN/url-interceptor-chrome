{
  "manifest_version": 2,
  "name": "Zerx Url Interceptor",
  "description": "This extension shows movies from zerx.co",
  "version": "0.1",
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },

  "background": {
    "scripts": ["scripts/libs/jquery-2.1.4.min.js", "scripts/background.js"]
  },

  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
